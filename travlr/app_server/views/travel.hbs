{{> header }}

<div id="contents">
  <div class="box">
    <div>
      <div class="body">
        <h1>Travel</h1>
        <ul id="sites">
          {{#each trips}}
            <li>
              <img src="{{image}}" alt="{{name}}">
              <h2>{{name}}</h2>
              <p>{{description}}</p>
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

{{> footer }}
